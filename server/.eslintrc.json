{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-case-declarations": "off",
    "no-empty-pattern": "off",
    "prefer-const": ["warn"]
  }
}
